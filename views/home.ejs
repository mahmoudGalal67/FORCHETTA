<script>
// Slider

const leftArrow_client = document.querySelector(".left-arrow.homeleftArrow")
const rightArrow_client = document.querySelector(".right-arrow.homeRightArrow")
const slider = document.querySelector(".clients-slider")
let sliderIndex=0 

leftArrow_client.addEventListener("click" ,()=>{
    if(sliderIndex!==-5){
      sliderIndex-=1
      slider.style.transform=`translateX(${410*sliderIndex}px)`
    }
})

rightArrow_client.addEventListener("click" ,()=>{
    if(sliderIndex!==0){
      sliderIndex+=1
      slider.style.transform=`translateX(${410*sliderIndex}px)`
    }
})

</script>

