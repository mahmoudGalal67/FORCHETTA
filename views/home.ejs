              <span class="empty-message">No item has been added to the gallary</span>
              <% }else{ %>
              <% data.menu.forEach((item)=>{%>
                <div class="gallary-item" style="min-width:calc(100%/3 - 16px);">
                  <div class="image-container" style="height:100%;">
                    <img src="uploads/<%= item.image %>" style="object-fit:cover;" alt="">
                  </div>
                  <span><%= item.name %></span>
                </div>
              <%}) }%>
          </div>
        </div>
      </section>
